<launch4jConfig>
  <dontWrapJar>true</dontWrapJar>
  <headerType>0</headerType>
  <jar>flashteacher.jar</jar>
  <outfile>FlashTeacher.exe</outfile>
  <errTitle></errTitle>
  <jarArgs></jarArgs>
  <chdir>.</chdir>
  <customProcName>true</customProcName>
  <stayAlive>false</stayAlive>
  <icon>src\com\ben\flashteacher\resources\flashteacher.ico</icon>
  <jre>
    <minVersion>1.8.0</minVersion>
    <maxVersion></maxVersion>
    <initialHeapSize>0</initialHeapSize>
    <maxHeapSize>0</maxHeapSize>
  </jre>
  <splash>
    <file>launch4j_loading_splash_screen.bmp</file>
    <waitForWindow>true</waitForWindow>
  </splash>
  <versionInfo>
    <fileVersion>1.1.0.0</fileVersion>
    <productVersion>1.1.0.0</productVersion>
    <txtFileVersion>1.1</txtFileVersion>
    <txtProductVersion>1.1</txtProductVersion>
    <copyright>(C) Ben Spiller 2007-2020</copyright>
    
    <fileDescription>FlashTeacher executable wrapper</fileDescription>
    <productName>FlashTeacher</productName>
    <companyName></companyName>
    <internalName>FlashTeacher</internalName>
    <originalFilename>FlashTeacher.exe</originalFilename>
  </versionInfo>
</launch4jConfig>